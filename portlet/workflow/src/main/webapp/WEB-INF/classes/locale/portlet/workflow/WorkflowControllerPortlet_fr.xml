<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #			org.exoplatform.workflow.webui.component.controller										#
  #############################################################################
  -->
  <UIWorkflowControllerPortlet>
    <tab>
      <label>Contrôleur</label>
      <text>Gestion du Workflow</text>
    </tab>
    <msg>
      <refresh-session-success>La session a été rafraichie avec succès</refresh-session-success>
    </msg>
    <label>
      <refesh>Rafraichir</refesh>
    </label>
  </UIWorkflowControllerPortlet>
  <ControllerPopup>
    <title>
      <UITaskManager>Gestion des tâches</UITaskManager>
    </title>
  </ControllerPopup>
  <UITabPane>
    <title>
      <UITaskList>Contrôleur des Tâches</UITaskList>
      <UIBPDefinition>Definition du Processus Métier</UIBPDefinition>
      <UIDocumentContent>Visualiser le contenu</UIDocumentContent>
      <UIDocumentForm>Editer le contenu</UIDocumentForm>
      <UITask>Tâche</UITask>
    </title>
  </UITabPane>
  <UITaskList>
    <header>
      <id>ID Tâche</id>
      <processName>Nom Processus</processName>
      <taskName>Nom Tâche</taskName>
      <actor>Acteur</actor>
      <description>Description</description>
      <actorId>Id Acteur</actorId>
      <taskVariables>Variables de la Tâche</taskVariables>
      <processDateStart>Date de début du Processus</processDateStart>
      <action>Action</action>
    </header>
    <label>
      <managerState>Gérer</managerState>
    </label>
    <msg>
      <task-not-found>La tâche ne peut pas être gérée. La raison la plus probable est qu'un autre utilisateur a déjà géré la tâche.</task-not-found>
    </msg>
  </UITaskList>
  <UITaksList>
    <msg>
      <no-task>Aucune tâche assignée</no-task>
    </msg>
  </UITaksList>
  <UITask>
    <label>
      <hide-process-link>Cacher le lien vers le processus</hide-process-link>
      <show-process-link>Montrer le lien vers le processus</show-process-link>
    </label>
    <msg>
      <has-not-got-delegate>Veuillez choisir un utilisateur à qui déléguer</has-not-got-delegate>
    </msg>
    <title>
      <Delegate>Saisir un utilisateur</Delegate>
    </title>
  </UITask>
  <UIBPDefinition>
    <header>
      <id>Id</id>
      <name>Nom</name>
      <version>Version</version>
      <action>Action</action>
    </header>
    <msg>
      <empty>Aucun Processus Métier à lancer</empty>
    </msg>
  </UIBPDefinition>
  <UIControllerManager>
    <action>
      <title>
        <ManageStart>Gestion du lancement</ManageStart>
      </title>
    </action>
  </UIControllerManager>
  <UIDocumentForm>
    <msg>
      <cannot-save>Certaines contraintes n'ont pas été respectées durant cette action.</cannot-save>
      <in-versioning>Veuillez extraire le noeud courant avant d'ajouter un nouveau noeud</in-versioning>
      <not-support>Impossible de charger le formulaire '{0}'.</not-support>
      <value-invalid>'{0}' est une valeur requise</value-invalid>
    </msg>
  </UIDocumentForm>
  <!--
  #####################################################################
  ## Need to be translated Since version ECM 2.3                      #
  #####################################################################
  -->
  
  <PopupDelegatedSelectorId>
    <title>
      <UIUserSelectContainer>Saisir un utilisateur</UIUserSelectContainer>
    </title>
  </PopupDelegatedSelectorId>
  
</bundle>

